package deinslaif;


public class Bird {
    
      
    
    public Bird () {
        System.out.println("unknown"); //if there is no bird name was declared
    }
    public Bird (String x, String y, String z){
    
        System.out.println("Birds name = "+x+" & "+y+" & "+z);
    }
    public static void main (String[] arg){
    
        Bird pakhi = new Bird("bird1", "bird2", "bird3"); //if theres bird name was declared
        
    }
}
